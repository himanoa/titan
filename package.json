{
  "name": "titan",
  "version": "0.0.1",
  "description": "Titan is better markdown editor",
  "main": "index.js",
  "author": "himanoa <matsunoappy@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts' 'src/**/*.tsx' index.ts",
    "lint:fix": "eslint --fix -c .eslintrc.js 'src/**/*.ts' 'src/**/*.tsx' 'src/*.ts' 'src/*.tsx'",
    "prettie": "prettier --check --config ./.prettierrc '**/*{ts,tsx}'",
    "prettier:fix": "prettier --write --config ./.prettierrc '**/*{ts,tsx}'",
    "tsc:watch": "tsc --watch --noEmit",
    "build:dev": "webpack --config ./webpack.config.dev.js",
    "electron": "electron dist/main.js",
    "start": "yarn build:dev && yarn electron"
  },
  "dependencies": {
    "electron": "^5.0.7",
    "monaco-editor": "^0.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/electron": "^1.6.10",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-monaco-editor": "^0.16.0",
    "@types/styled-components": "^4.1.18",
    "@types/webpack": "^4.32.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-loader": "8.0.4",
    "css-loader": "^3.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^22.12.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.3",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "react-monaco-editor": "^0.27.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  }
